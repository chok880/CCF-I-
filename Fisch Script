local Player = game:GetService("Players")
local LocalPlayer = Player.LocalPlayer
local Char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local Humanoid = Char:WaitForChild("Humanoid")
local VirtualInputManager = game:GetService("VirtualInputManager")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local GuiService = game:GetService("GuiService")
local UserInputService = game:GetService("UserInputService")

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
LocalPlayer.CharacterAdded:Connect(function(c)
    Char = c
    Humanoid = Char:WaitForChild("Humanoid")
end)

local function equipitem(v)
    if LocalPlayer.Backpack:FindFirstChild(v) then
        Humanoid:EquipTool(LocalPlayer.Backpack[v])
    end
end

-- ‡πÇ‡∏´‡∏•‡∏î Discord UI
local DiscordLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord"))()
local win = DiscordLib:Window("CHOK HUB üé£")
local serv = win:Server("‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ï‡∏Å‡∏õ‡∏•‡∏≤ Fisch", "")

local main = serv:Channel("‡πÄ‡∏°‡∏ô‡∏π‡∏´‡∏•‡∏±‡∏Å")

------------------------------------------------
-- üéØ‚ö°üé£ Auto Fish
main:Toggle("üéØ‚ö°üé£ ‡∏õ‡∏•‡∏≤‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", false, function(v)
    _G.AutoFishAll = v
    pcall(function()
        while _G.AutoFishAll do
            task.wait(0.1)

            -- AutoCast
            local Rod = Char:FindFirstChildOfClass("Tool")
            if Rod and Rod:FindFirstChild("events") and Rod.events:FindFirstChild("cast") then
                Rod.events.cast:FireServer(100, 1, false, Vector3.new(0,0,0), tick(), "Auto")
            end

            -- AutoShake
            local shakeUI = LocalPlayer.PlayerGui:FindFirstChild("shakeui")
            if shakeUI and shakeUI.Enabled then
                local safezone = shakeUI:FindFirstChild("safezone")
                if safezone then
                    local button = safezone:FindFirstChild("button")
                    if button and button:IsA("ImageButton") and button.Visible then
                        GuiService.SelectedObject = button
                        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
                        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
                    end
                end
            end

            -- AutoReel
            for _, gui in pairs(LocalPlayer.PlayerGui:GetChildren()) do
                if gui:IsA("ScreenGui") and gui.Name == "reel" and gui:FindFirstChild("bar") then
                    ReplicatedStorage.events.reelfinished:FireServer(100, true, false, Vector3.new(0,0,0), tick(), "Auto")
                end
            end

            -- Auto Equip Rod
            for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
                if tool:IsA("Tool") and tool.Name:lower():find("rod") then
                    equipitem(tool.Name)
                end
            end
        end
    end)
end)

------------------------------------------------
-- üìç ‡∏¢‡∏∑‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏î‡∏¥‡∏°
main:Toggle("üìç ‡∏¢‡∏∑‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏î‡∏¥‡∏°", false, function(v)
    _G.AutoReturn = v
    if v then
        local startPos = Char.HumanoidRootPart.Position
        while _G.AutoReturn do
            task.wait(0.2)
            if (Char.HumanoidRootPart.Position - startPos).Magnitude > 0.02 then
                Char.HumanoidRootPart.CFrame = CFrame.new(startPos)
            end
        end
    end
end)

------------------------------------------------
-- ‚è≥ Anti AFK
main:Toggle("‚è≥ ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô AFK (Anti AFK)", false, function(v)
    _G.AntiAFK = v
    if v then
        spawn(function()
            while _G.AntiAFK do
                VirtualInputManager:SendMouseButtonEvent(0,0,true,game)
                task.wait(0.1)
                VirtualInputManager:SendMouseButtonEvent(0,0,false,game)
            end
        end)
    end
end)

------------------------------------------------
-- üöÄ ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞‡∏ï‡πà‡∏≤‡∏á ‡πÜ
local warp = serv:Channel("‡∏ß‡∏≤‡∏õ")
warp:Label("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Å‡∏≤‡∏∞‡πÅ‡∏•‡πâ‡∏ß‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ß‡∏≤‡∏õ")

-- üó∫ ‡πÉ‡∏™‡πà‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏à‡∏£‡∏¥‡∏á‡∏ó‡∏µ‡πà‡∏Ñ‡∏∏‡∏ì‡πÑ‡∏î‡πâ‡∏à‡∏≤‡∏Å‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏î‡∏∂‡∏á‡∏û‡∏¥‡∏Å‡∏±‡∏î
local Islands = {
    ["üèù ‡πÄ‡∏Å‡∏≤‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô"] = CFrame.new(0, 10, 0), -- ‡πÉ‡∏™‡πà‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏à‡∏£‡∏¥‡∏á
    ["üå¥ ‡πÄ‡∏Å‡∏≤‡∏∞‡∏õ‡πà‡∏≤"] = CFrame.new(500, 15, 300), -- ‡πÉ‡∏™‡πà‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏à‡∏£‡∏¥‡∏á
    ["‚ùÑÔ∏è ‡πÄ‡∏Å‡∏≤‡∏∞‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á"] = CFrame.new(-800, 20, 1200), -- ‡πÉ‡∏™‡πà‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏à‡∏£‡∏¥‡∏á
    ["üåã ‡πÄ‡∏Å‡∏≤‡∏∞‡∏†‡∏π‡πÄ‡∏Ç‡∏≤‡πÑ‡∏ü"] = CFrame.new(1500, 25, -400), -- ‡πÉ‡∏™‡πà‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏à‡∏£‡∏¥‡∏á
    ["üêâ ‡πÄ‡∏Å‡∏≤‡∏∞‡∏•‡∏±‡∏ö"] = CFrame.new(-2000, 30, -1800) -- ‡πÉ‡∏™‡πà‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏à‡∏£‡∏¥‡∏á
}

local selectedIsland = nil
warp:Dropdown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Å‡∏≤‡∏∞", table.keys(Islands), function(v)
    selectedIsland = v
end)

warp:Button("üöÄ ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞", function()
    if selectedIsland and Islands[selectedIsland] and Char and Char:FindFirstChild("HumanoidRootPart") then
        Char.HumanoidRootPart.CFrame = Islands[selectedIsland]
    end
end)

------------------------------------------------
-- ‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï
local credit = serv:Channel("‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï")
credit:Label("üíª ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÇ‡∏î‡∏¢ CHOK HUB")
credit:Button("‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π", function()
    DiscordLib:Close()
end)

