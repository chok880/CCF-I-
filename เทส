-- ‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö V.2.16 - ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡πÅ‡∏•‡πâ‡∏ß + ‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î
-- ‡πÄ‡∏ü‡∏ã‡∏ö‡∏∏‡πä‡∏Ñ: ‡∏°‡∏≤‡∏õ‡∏±‡πà‡∏ô‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢
-- üìå ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡∏ó‡∏∏‡∏Å‡∏ö‡∏±‡∏Ñ + ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î + ‡πÑ‡∏°‡πà‡∏°‡∏µ Error

------------------------------------------------------------
-- üìå ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡πÅ‡∏•‡πâ‡∏ß)
------------------------------------------------------------
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TeleportService = game:GetService("TeleportService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")

local LocalPlayer = Players.LocalPlayer

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏≠ Character ‡∏ó‡∏µ‡πà‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
local function waitForCharacter()
    local char = LocalPlayer.Character
    if not char then
        char = LocalPlayer.CharacterAdded:Wait()
    end
    return char
end

local Char = waitForCharacter()

-- ‡πÇ‡∏´‡∏•‡∏î UI ‡πÅ‡∏ö‡∏ö‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢ (‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πâ HTTP)
local Luna
local success = pcall(function()
    local lunaUrl = "https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua"
    Luna = loadstring(game:HttpGet(lunaUrl))()
end)

if not success or not Luna then
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏£‡∏≠‡∏á‡πÅ‡∏ó‡∏ô
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    
    ScreenGui.Name = "CK_HUB_Emergency"
    ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
    
    Frame.Size = UDim2.new(0, 400, 0, 300)
    Frame.Position = UDim2.new(0.5, -200, 0.5, -150)
    Frame.BackgroundColor3 = Color3.new(0, 0, 0)
    Frame.BackgroundTransparency = 0.2
    Frame.BorderSizePixel = 2
    Frame.BorderColor3 = Color3.new(1, 1, 1)
    Frame.Parent = ScreenGui
    
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.BackgroundTransparency = 1
    TextLabel.Text = "‚ö†Ô∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏ä‡πâ‡πÇ‡∏´‡∏°‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô\nüöÄ ‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö V.2.16\n‚úÖ ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡πÅ‡∏•‡πâ‡∏ß\n\nüéÆ ‡∏Å‡∏î F9 ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏î‡∏π‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á"
    TextLabel.TextColor3 = Color3.new(1, 1, 1)
    TextLabel.TextScaled = true
    TextLabel.Font = Enum.Font.GothamBold
    TextLabel.Parent = Frame
    
    print("üö® ‡πÉ‡∏ä‡πâ‡πÇ‡∏´‡∏°‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô - Luna UI ‡πÇ‡∏´‡∏•‡∏î‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ")
    print("üìã ‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡πÑ‡∏î‡πâ:")
    print("_G.StartAutoFarm() - ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°")
    print("_G.StopAutoFarm() - ‡∏´‡∏¢‡∏∏‡∏î‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°")
    print("_G.SetWalkSpeed(‡∏Ñ‡πà‡∏≤) - ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß")
    print("_G.SaveFarmPos() - ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°")
    print("_G.SaveWaterPos() - ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ô‡πâ‡∏≥")
    print("_G.SaveSellPos() - ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏Ç‡∏≤‡∏¢")
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏£‡∏∞‡∏ö‡∏ö‡πÅ‡∏ó‡∏ô Luna
    _G.CK_HUB_Emergency = true
else
    -- ‡πÉ‡∏ä‡πâ Luna UI ‡∏õ‡∏Å‡∏ï‡∏¥
    local Window = Luna:CreateWindow({
        Name = "‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö ‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö",
        Subtitle = "V.2.16 - ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡πÅ‡∏•‡πâ‡∏ß",
        LoadingEnabled = true,
        LoadingTitle = "‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö ‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö",
        LoadingSubtitle = "‡πÇ‡∏î‡∏¢ ‡πÄ‡∏ü‡∏ã‡∏ö‡∏∏‡πä‡∏Ñ ‡∏°‡∏≤‡∏õ‡∏±‡πà‡∏ô‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢",
        ConfigSettings = {
            ConfigFolder = "CK_HUB_Config"
        },
        KeySystem = false
    })

    local Tab_Main = Window:CreateTab({
        Name = "‡πÄ‡∏°‡∏ô‡∏π‡∏´‡∏•‡∏±‡∏Å ‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö",
        Icon = "flash_on",
        ImageSource = "Material"
    })
end

------------------------------------------------------------
-- üìç ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏Ñ‡∏±‡∏ç
------------------------------------------------------------
local farmPos, waterPos, sellPos = nil, nil, nil
local autoFarmRunning = false
local antiAfkRunning = false

------------------------------------------------------------
-- üîß ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡πÅ‡∏•‡πâ‡∏ß)
------------------------------------------------------------

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
local function safeTeleport(position)
    if not position then return end
    pcall(function()
        local currentChar = waitForCharacter()
        if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
            currentChar.HumanoidRootPart.CFrame = position
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Pan
local function isPanTool(toolName)
    if not toolName then return false end
    return string.sub(toolName:lower(), -3) == "pan"
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡∏≤‡∏¢‡∏Ç‡∏≠‡∏á
local function autoSell()
    pcall(function()
        local sellRemote = ReplicatedStorage:WaitForChild("Remotes", 5)
        if sellRemote then
            local shopRemote = sellRemote:WaitForChild("Shop", 2)
            if shopRemote then
                local sellAllRemote = shopRemote:WaitForChild("SellAll", 2)
                if sellAllRemote then
                    sellAllRemote:InvokeServer()
                    print("üí∞ ‡∏Ç‡∏≤‡∏¢‡∏Ç‡∏≠‡∏á‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!")
                end
            end
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏´‡∏•‡∏±‡∏Å
local function startAutoFarm()
    autoFarmRunning = true
    print("üöÄ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö!")
    
    task.spawn(function()
        while autoFarmRunning do
            pcall(function()
                local currentChar = waitForCharacter()
                if not currentChar then return end
                
                -- ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Å‡∏±‡∏ö Pan Tools
                local containers = {currentChar, LocalPlayer.Backpack}
                for _, container in pairs(containers) do
                    if container then
                        for _, tool in pairs(container:GetChildren()) do
                            if tool:IsA("Tool") and isPanTool(tool.Name) then
                                local scriptsFolder = tool:FindFirstChild("Scripts")
                                if scriptsFolder then
                                    -- Collect
                                    task.spawn(function()
                                        local collect = scriptsFolder:FindFirstChild("Collect")
                                        if collect and collect:IsA("RemoteFunction") then
                                            pcall(function()
                                                collect:InvokeServer(1)
                                            end)
                                        end
                                    end)

                                    -- Shake
                                    task.spawn(function()
                                        local shake = scriptsFolder:FindFirstChild("Shake")
                                        if shake and shake:IsA("RemoteEvent") then
                                            pcall(function()
                                                shake:FireServer()
                                            end)
                                        end
                                    end)

                                    -- Pan (‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏ï‡πá‡∏°)
                                    task.spawn(function()
                                        local gui = LocalPlayer.PlayerGui:FindFirstChild("ToolUI")
                                        if gui and gui:FindFirstChild("FillingPan") then
                                            local fillText = gui.FillingPan:FindFirstChild("FillText")
                                            if fillText and fillText.Text then
                                                local current, max = fillText.Text:match("([%d%.]+)/([%d%.]+)")
                                                current, max = tonumber(current), tonumber(max)
                                                
                                                if current and max and current >= max then
                                                    local pan = scriptsFolder:FindFirstChild("Pan")
                                                    if pan and pan:IsA("RemoteFunction") then
                                                        pcall(function()
                                                            pan:InvokeServer()
                                                        end)
                                                    end
                                                end
                                            end
                                        end
                                    end)
                                end
                            end
                        end
                    end
                end

                -- ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
                task.spawn(function()
                    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡πÄ‡∏ï‡πá‡∏°
                    local backpackGui = LocalPlayer.PlayerGui:FindFirstChild("BackpackGui")
                    if backpackGui then
                        local invPath = backpackGui:FindFirstChild("Backpack")
                        if invPath and invPath:FindFirstChild("Inventory") then
                            local topButtons = invPath.Inventory:FindFirstChild("TopButtons")
                            if topButtons and topButtons:FindFirstChild("Unaffected") then
                                local invSize = topButtons.Unaffected:FindFirstChild("InventorySize")
                                if invSize and invSize.Text then
                                    local invCur, invMax = invSize.Text:match("(%d+)/(%d+)")
                                    invCur, invMax = tonumber(invCur), tonumber(invMax)

                                    if invCur and invMax and invCur >= invMax and sellPos then
                                        print("üí∞ ‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡πÄ‡∏ï‡πá‡∏°! ‡πÑ‡∏õ‡∏Ç‡∏≤‡∏¢...")
                                        safeTeleport(sellPos)
                                        task.wait(0.5)
                                        autoSell()
                                        task.wait(0.5)
                                        return
                                    end
                                end
                            end
                        end
                    end

                    -- ‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°/‡∏ô‡πâ‡∏≥
                    local toolGui = LocalPlayer.PlayerGui:FindFirstChild("ToolUI")
                    if toolGui and toolGui:FindFirstChild("FillingPan") then
                        local fillText = toolGui.FillingPan:FindFirstChild("FillText")
                        if fillText and fillText.Text and waterPos and farmPos then
                            local current, max = fillText.Text:match("([%d%.]+)/([%d%.]+)")
                            current, max = tonumber(current), tonumber(max)

                            if current and max then
                                if current >= max then
                                    safeTeleport(waterPos)
                                    print("üíß ‡πÑ‡∏õ‡∏•‡πâ‡∏≤‡∏á‡πÅ‡∏£‡πà...")
                                else
                                    safeTeleport(farmPos)
                                    print("‚õèÔ∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ü‡∏≤‡∏£‡πå‡∏°... ("..current.."/"..max..")")
                                end
                            end
                        end
                    end
                end)
            end)
            
            task.wait(0) -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î!
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏¢‡∏∏‡∏î‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°
local function stopAutoFarm()
    autoFarmRunning = false
    print("‚èπÔ∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÅ‡∏•‡πâ‡∏ß")
end

------------------------------------------------------------
-- üéÆ UI Controls (‡∏ñ‡πâ‡∏≤‡πÇ‡∏´‡∏•‡∏î Luna ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à)
------------------------------------------------------------
if Luna and Tab_Main then
    -- ‡∏õ‡∏∏‡πà‡∏°‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
    Tab_Main:CreateButton({
        Name = "üìç ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°",
        Callback = function()
            local currentChar = waitForCharacter()
            if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
                farmPos = currentChar.HumanoidRootPart.CFrame
                print("üìç ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÅ‡∏•‡πâ‡∏ß!")
            end
        end
    })

    Tab_Main:CreateButton({
        Name = "üíß ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ô‡πâ‡∏≥",
        Callback = function()
            local currentChar = waitForCharacter()
            if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
                waterPos = currentChar.HumanoidRootPart.CFrame
                print("üíß ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ô‡πâ‡∏≥‡πÅ‡∏•‡πâ‡∏ß!")
            end
        end
    })

    Tab_Main:CreateButton({
        Name = "üí∞ ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏Ç‡∏≤‡∏¢",
        Callback = function()
            local currentChar = waitForCharacter()
            if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
                sellPos = currentChar.HumanoidRootPart.CFrame
                print("üí∞ ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏Ç‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß!")
            end
        end
    })

    -- ‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°
    Tab_Main:CreateToggle({
        Name = "üöÄ ‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö (‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î)",
        CurrentValue = false,
        Callback = function(state)
            if state then
                startAutoFarm()
            else
                stopAutoFarm()
            end
        end
    })

    -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
    Tab_Main:CreateSlider({
        Name = "üí® ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î",
        Range = {16, 1000},
        Increment = 10,
        CurrentValue = 16,
        Callback = function(value)
            task.spawn(function()
                while task.wait(0.1) do
                    local currentChar = waitForCharacter()
                    if currentChar and currentChar:FindFirstChild("Humanoid") then
                        currentChar.Humanoid.WalkSpeed = value
                    end
                    if not autoFarmRunning then break end
                end
            end)
        end
    })

    -- Anti-AFK
    Tab_Main:CreateToggle({
        Name = "‚ö° ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î (‡πÄ‡∏£‡πá‡∏ß)",
        CurrentValue = false,
        Callback = function(state)
            antiAfkRunning = state
            if state then
                task.spawn(function()
                    while antiAfkRunning do
                        pcall(function()
                            VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.LeftShift, false, game)
                            task.wait(0.01)
                            VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.LeftShift, false, game)
                        end)
                        task.wait(30)
                    end
                end)
            end
        end
    })

    -- ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î
    Tab_Main:CreateButton({
        Name = "üéØ ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î",
        Callback = function()
            local codes = {
                "coolupdate", "hundredmillion", "traveler", "fossilized", 
                "sorrytwo", "millions", "updateone"
            }

            task.spawn(function()
                for _, code in pairs(codes) do
                    pcall(function()
                        local claimRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Misc"):WaitForChild("ClaimCode")
                        claimRemote:FireServer(code)
                        print("‚úÖ ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î: " .. code)
                    end)
                    task.wait(0.1)
                end
                print("üéâ ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß!")
            end)
        end
    })
end

------------------------------------------------------------
-- üåê Global Functions (‡πÉ‡∏ä‡πâ‡πÑ‡∏î‡πâ‡∏ó‡∏∏‡∏Å‡πÇ‡∏´‡∏°‡∏î)
------------------------------------------------------------
_G.StartAutoFarm = startAutoFarm
_G.StopAutoFarm = stopAutoFarm

_G.SaveFarmPos = function()
    local currentChar = waitForCharacter()
    if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
        farmPos = currentChar.HumanoidRootPart.CFrame
        print("üìç ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÅ‡∏•‡πâ‡∏ß!")
    end
end

_G.SaveWaterPos = function()
    local currentChar = waitForCharacter()
    if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
        waterPos = currentChar.HumanoidRootPart.CFrame
        print("üíß ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ô‡πâ‡∏≥‡πÅ‡∏•‡πâ‡∏ß!")
    end
end

_G.SaveSellPos = function()
    local currentChar = waitForCharacter()
    if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
        sellPos = currentChar.HumanoidRootPart.CFrame
        print("üí∞ ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏Ç‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß!")
    end
end

_G.SetWalkSpeed = function(speed)
    local currentChar = waitForCharacter()
    if currentChar and currentChar:FindFirstChild("Humanoid") then
        currentChar.Humanoid.WalkSpeed = speed
        print("üí® ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß: " .. speed)
    end
end

------------------------------------------------------------
-- üîÑ ‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£ Character ‡πÉ‡∏´‡∏°‡πà
------------------------------------------------------------
LocalPlayer.CharacterAdded:Connect(function(newChar)
    task.wait(1)
    Char = newChar
