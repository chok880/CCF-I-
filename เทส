-- ‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö V.2.15 - ‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î + ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ
-- ‡πÄ‡∏ü‡∏ã‡∏ö‡∏∏‡πä‡∏Ñ: ‡∏°‡∏≤‡∏õ‡∏±‡πà‡∏ô‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢ (‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÅ‡∏•‡πâ‡∏ß)
-- üìå ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î + ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ + ‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡∏õ‡∏£‡∏∞‡∏™‡∏¥‡∏ó‡∏ò‡∏¥‡∏†‡∏≤‡∏û

------------------------------------------------------------
-- üìå ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô + ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ
------------------------------------------------------------
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TeleportService = game:GetService("TeleportService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local RunService = game:GetService("RunService")

local LocalPlayer = Players.LocalPlayer

-- ‡∏ü‡∏¥‡∏Å‡∏ã‡πå‡∏Å‡∏≤‡∏£‡πÇ‡∏´‡∏•‡∏î Character
local function getChar()
    return LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
end

local Char = getChar()

-- ‡πÇ‡∏´‡∏•‡∏î Luna UI ‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î
local Luna
pcall(function()
    Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua"))()
end)

if not Luna then
    warn("‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÇ‡∏´‡∏•‡∏î Luna UI ‡πÑ‡∏î‡πâ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏ä‡πâ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏≥‡∏£‡∏≠‡∏á...")
    return
end

local Window = Luna:CreateWindow({
    Name = "‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö ‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö",
    Subtitle = "V.2.15 - ‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î",
    LoadingEnabled = true,
    LoadingTitle = "‡∏ã‡∏µ‡πÄ‡∏Ñ ‡∏Æ‡∏±‡∏ö ‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö",
    LoadingSubtitle = "‡πÇ‡∏î‡∏¢ ‡πÄ‡∏ü‡∏ã‡∏ö‡∏∏‡πä‡∏Ñ ‡∏°‡∏≤‡∏õ‡∏±‡πà‡∏ô‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢ - ‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÅ‡∏•‡πâ‡∏ß",
    ConfigSettings = {
        ConfigFolder = "CK HUB TURBO Config"
    },
    KeySystem = false
})

Window:CreateHomeTab({
    SupportedExecutors = {
        "Synapse X","Krnl","Fluxus","Script-Ware","xeno","Velocity",
        "Delta","JJSploit","Proxo","EasyExploits","OxygenU",
        "Comet","Electron","Arsenal","Calamari","Wave","Solara"
    },
    Icon = 1
})

local Tab_Main = Window:CreateTab({
    Name = "‡πÄ‡∏°‡∏ô‡∏π‡∏´‡∏•‡∏±‡∏Å ‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö",
    Icon = "flash_on",
    ImageSource = "Material"
})

------------------------------------------------------------
-- üìç ‡∏õ‡∏∏‡πà‡∏°‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á (‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÅ‡∏•‡πâ‡∏ß)
------------------------------------------------------------
local farmPos, waterPos, sellPos = nil, nil, nil
local statusLabel

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
local function savePosition(posType)
    pcall(function()
        Char = getChar()
        if Char and Char:FindFirstChild("HumanoidRootPart") then
            local pos = Char.HumanoidRootPart.CFrame
            if posType == "farm" then
                farmPos = pos
                print("üìç ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ü‡∏≤‡∏£‡πå‡∏°:", pos)
            elseif posType == "water" then
                waterPos = pos
                print("üíß ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ô‡πâ‡∏≥:", pos)
            elseif posType == "sell" then
                sellPos = pos
                print("üí∞ ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≤‡∏¢:", pos)
            end
        end
    end)
end

Tab_Main:CreateButton({
    Name = "üìç ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°",
    Callback = function()
        savePosition("farm")
    end
})

Tab_Main:CreateButton({
    Name = "üíß ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ô‡πâ‡∏≥", 
    Callback = function()
        savePosition("water")
    end
})

Tab_Main:CreateButton({
    Name = "üí∞ ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏Ç‡∏≤‡∏¢",
    Callback = function()
        savePosition("sell")
    end
})

------------------------------------------------------------
-- üöÄ ‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏° TURBO MODE (‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î)
------------------------------------------------------------
statusLabel = Tab_Main:CreateLabel({
    Text = "‚ö° ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö TURBO MODE...",
    Style = 2
})

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
_G.TurboMode = false
_G.AutoFarm = false

Tab_Main:CreateToggle({
    Name = "üöÄ ‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏ö (‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î)",
    CurrentValue = false,
    Callback = function(state)
        _G.AutoFarm = state
        _G.TurboMode = state
        
        if state then
            task.spawn(function()
                local function isPan(toolName)
                    return toolName and string.sub(toolName:lower(), -3) == "pan"
                end

                local function safeTeleport(position)
                    pcall(function()
                        Char = getChar()
                        if Char and Char:FindFirstChild("HumanoidRootPart") and position then
                            Char.HumanoidRootPart.CFrame = position
                        end
                    end)
                end

                local function autoSell()
                    pcall(function()
                        ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Shop"):WaitForChild("SellAll"):InvokeServer()
                        statusLabel:Set("üí∞ ‡∏Ç‡∏≤‡∏¢‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠...")
                    end)
                end

                while _G.AutoFarm do
                    Char = getChar()
                    
                    -- TURBO PAN OPERATIONS (‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î)
                    local containers = {Char, LocalPlayer.Backpack}
                    for _, container in pairs(containers) do
                        if container then
                            for _, tool in pairs(container:GetChildren()) do
                                if tool:IsA("Tool") and isPan(tool.Name) then
                                    local scriptsFolder = tool:FindFirstChild("Scripts")
                                    if scriptsFolder then
                                        -- ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏ö‡∏ö‡∏Ç‡∏ô‡∏≤‡∏ô (Parallel Processing)
                                        task.spawn(function()
                                            local collect = scriptsFolder:FindFirstChild("Collect")
                                            if collect and collect:IsA("RemoteFunction") then
                                                pcall(function()
                                                    collect:InvokeServer(1)
                                                end)
                                            end
                                        end)

                                        task.spawn(function()
                                            local shake = scriptsFolder:FindFirstChild("Shake")
                                            if shake and shake:IsA("RemoteEvent") then
                                                pcall(function()
                                                    shake:FireServer()
                                                end)
                                            end
                                        end)

                                        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏≤‡∏£‡∏•‡πâ‡∏≤‡∏á‡πÅ‡∏£‡πà
                                        local gui = LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("ToolUI")
                                        if gui and gui:FindFirstChild("FillingPan") then
                                            local fillText = gui.FillingPan:FindFirstChild("FillText")
                                            if fillText and fillText.Text then
                                                local current, max = fillText.Text:match("([%d%.]+)/([%d%.]+)")
                                                current, max = tonumber(current), tonumber(max)
                                                
                                                if current and max and current >= max then
                                                    local pan = scriptsFolder:FindFirstChild("Pan")
                                                    if pan and pan:IsA("RemoteFunction") then
                                                        task.spawn(function()
                                                            pcall(function()
                                                                pan:InvokeServer()
                                                            end)
                                                        end)
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end

                    -- TURBO MOVEMENT & AUTO SELL
                    pcall(function()
                        local gui = LocalPlayer.PlayerGui:FindFirstChild("ToolUI")
                        local backpackGui = LocalPlayer.PlayerGui:FindFirstChild("BackpackGui")
                        
                        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡πÄ‡∏ï‡πá‡∏°
                        if backpackGui and backpackGui:FindFirstChild("Backpack") then
                            local invSizeLabel = backpackGui.Backpack:FindFirstChild("Inventory")
                            if invSizeLabel and invSizeLabel:FindFirstChild("TopButtons") then
                                local sizeText = invSizeLabel.TopButtons.Unaffected.InventorySize.Text
                                local invCur, invMax = sizeText:match("(%d+)/(%d+)")
                                invCur, invMax = tonumber(invCur), tonumber(invMax)

                                if invCur and invMax and invCur >= invMax and sellPos then
                                    statusLabel:Set("üöÄ TURBO: ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏Ç‡∏≤‡∏¢! ("..invCur.."/"..invMax..")")
                                    safeTeleport(sellPos)
                                    task.wait(0.1) -- ‡∏•‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≠
                                    autoSell()
                                    task.wait(0.1)
                                    return
                                end
                            end
                        end

                        -- TURBO FARM/WATER SWITCHING
                        if gui and gui:FindFirstChild("FillingPan") then
                            local fillText = gui.FillingPan:FindFirstChild("FillText")
                            if fillText and fillText.Text and waterPos and farmPos then
                                local current, max = fillText.Text:match("([%d%.]+)/([%d%.]+)")
                                current, max = tonumber(current), tonumber(max)

                                if current and max then
                                    if current >= max then
                                        safeTeleport(waterPos)
                                        statusLabel:Set("üöÄ TURBO: ‡∏•‡πà‡∏≠‡∏ô‡πÅ‡∏£‡πà! (‡πÄ‡∏ï‡πá‡∏°)")
                                    else
                                        safeTeleport(farmPos)
                                        statusLabel:Set("üöÄ TURBO: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ü‡∏≤‡∏£‡πå‡∏°! ("..current.."/"..max..")")
                                    end
                                end
                            end
                        end
                    end)

                    -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î - ‡πÑ‡∏°‡πà‡∏£‡∏≠‡πÄ‡∏•‡∏¢
                    task.wait(0)
                end

                statusLabel:Set("‚èπÔ∏è ‡∏´‡∏¢‡∏∏‡∏î TURBO MODE")
            end)
        end
    end
})

------------------------------------------------------------
-- üí® ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î
------------------------------------------------------------
local Humanoid = Char:WaitForChild("Humanoid")

Tab_Main:CreateSlider({
    Name = "üí® ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î (WalkSpeed)",
    Range = {16, 1000},
    Increment = 10,
    CurrentValue = Humanoid.WalkSpeed,
    Callback = function(value)
        task.spawn(function()
            while task.wait(0.1) do
                Char = getChar()
                if Char and Char:FindFirstChild("Humanoid") then
                    Char.Humanoid.WalkSpeed = value
                else
                    break
                end
                if not _G.AutoFarm then break end
            end
        end)
    end
})

Tab_Main:CreateSlider({
    Name = "üöÄ ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏∏‡∏î‡πÇ‡∏´‡∏î (JumpPower)", 
    Range = {50, 2000},
    Increment = 25,
    CurrentValue = Humanoid.JumpPower,
    Callback = function(value)
        task.spawn(function()
            while task.wait(0.1) do
                Char = getChar()
                if Char and Char:FindFirstChild("Humanoid") then
                    Char.Humanoid.JumpPower = value
                else
                    break
                end
                if not _G.AutoFarm then break end
            end
        end)
    end
})

------------------------------------------------------------
-- ‚ö° TURBO Anti-AFK (‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î‡πÅ‡∏ö‡∏ö‡πÄ‡∏£‡πá‡∏ß)
------------------------------------------------------------
Tab_Main:CreateToggle({
    Name = "‚ö° TURBO Anti-AFK (‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î‡πÄ‡∏£‡πá‡∏ß)",
    CurrentValue = false,
    Callback = function(state)
        _G.AntiAFK = state
        if state then
            task.spawn(function()
                while _G.AntiAFK do
                    pcall(function()
                        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.LeftShift, false, game)
                        task.wait(0.01)
                        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.LeftShift, false, game)
                    end)
                    task.wait(30) -- ‡∏•‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≠‡∏à‡∏≤‡∏Å 60 ‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 30 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
                end
            end)
        end
    end
})

------------------------------------------------------------
-- üéØ INSTANT CODE REDEEM (‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡πÅ‡∏ö‡∏ö‡πÄ‡∏£‡πá‡∏ß)
------------------------------------------------------------
Tab_Main:CreateButton({
    Name = "üéØ ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î (INSTANT)",
    Callback = function()
        local codes = {
            "coolupdate", "hundredmillion", "traveler", "fossilized", 
            "sorrytwo", "millions", "updateone", "newupdate", 
            "specialevent", "bonuscode", "freegems"
        }

        task.spawn(function()
            statusLabel:Set("üéØ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î INSTANT MODE...")
            local successCount = 0
            
            -- ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡πÅ‡∏ö‡∏ö‡∏Ç‡∏ô‡∏≤‡∏ô
            local threads = {}
            for _, code in ipairs(codes) do
                local thread = task.spawn(function()
                    pcall(function()
                        local ClaimCode = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Misc"):WaitForChild("ClaimCode")
                        ClaimCode:FireServer(code)
                        successCount = successCount + 1
                        print("‚úÖ ‡∏™‡πà‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à:", code)
                    end)
                end)
                table.insert(threads, thread)
            end
            
            -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ó‡∏∏‡∏Å thread ‡πÄ‡∏™‡∏£‡πá‡∏à
            task.wait(2)
            statusLabel:Set("üéâ ‡πÉ‡∏™‡πà‡πÇ‡∏Ñ‡πâ‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à! ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à: "..successCount.." ‡πÇ‡∏Ñ‡πâ‡∏î")
        end)
    end
})

------------------------------------------------------------
-- üîÑ INSTANT REJOIN (‡∏£‡∏µ‡∏à‡∏≠‡∏¢‡πÄ‡∏£‡πá‡∏ß)
------------------------------------------------------------
Tab_Main:CreateButton({
    Name = "üîÑ ‡∏£‡∏µ‡∏à‡∏≠‡∏¢‡πÅ‡∏°‡∏û (INSTANT)",
    Callback = function()
        statusLabel:Set("üîÑ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏£‡∏µ‡∏à‡∏≠‡∏¢...")
        task.spawn(function()
            pcall(function()
                TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, LocalPlayer)
            end)
        end)
    end
})

