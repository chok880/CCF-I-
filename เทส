------------------------------------------------------------
-- ü•Ñ ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á)
------------------------------------------------------------
statusLabel = Tab_Main:CreateLabel({
    Text = "‚ö†Ô∏è ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏£‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô...",
    Style = 2
})

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡∏ö‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥
local stats = {
    panCount = 0,
    collectCount = 0,
    shakeCount = 0
}

local statsLabel = Tab_Main:CreateLabel({
    Text = "üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥: Pan: 0 | Collect: 0 | Shake: 0",
    Style = 1
})

Tab_Main:CreateToggle({
    Name = "ü•Ñ ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÅ‡∏û‡∏ô + ‡∏ß‡∏≤‡∏£‡πå‡∏õ)",
    CurrentValue = false,
    Callback = function(state)
        _G.AutoPanWarp = state
        task.spawn(function()
            local function isPan(toolName)
                return string.sub(toolName, -3):lower() == "pan"
            end

            while _G.AutoPanWarp and task.wait() do
                Char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

                local containers = {Char, LocalPlayer.Backpack}
                for _, container in pairs(containers) do
                    for _, tool in pairs(container:GetChildren()) do
                        if tool:IsA("Tool") and isPan(tool.Name) then
                            local scriptsFolder = tool:FindFirstChild("Scripts")
                            if scriptsFolder then
                                local collect = scriptsFolder:FindFirstChild("Collect")
                                local pan = scriptsFolder:FindFirstChild("Pan")
                                local shake = scriptsFolder:FindFirstChild("Shake")

                                -- ‡∏î‡∏∂‡∏á‡∏Ñ‡πà‡∏≤‡∏´‡∏•‡∏≠‡∏î‡∏à‡∏≤‡∏Å GUI
                                local gui = LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("ToolUI")
                                local fillText = gui and gui:FindFirstChild("FillingPan") and gui.FillingPan:FindFirstChild("FillText")

                                local current, max = nil, nil
                                if fillText and fillText.Text then
                                    local a, b = fillText.Text:match("([%d%.]+)/([%d%.]+)")
                                    current, max = tonumber(a), tonumber(b)
                                end

                                -- ü•Ñ Collect
                                if collect and collect:IsA("RemoteFunction") then
                                    pcall(function()
                                        collect:InvokeServer(1)
                                        stats.collectCount = stats.collectCount + 1
                                    end)
                                end

                                -- ‚úÖ Pan ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏ï‡∏≠‡∏ô‡πÄ‡∏ï‡πá‡∏°
                                if pan and pan:IsA("RemoteFunction") and current and max and current >= max then
                                    pcall(function()
                                        pan:InvokeServer()
                                        stats.panCount = stats.panCount + 1
                                    end)
                                end

                                -- üåÄ Shake
                                if shake and shake:IsA("RemoteEvent") then
                                    pcall(function()
                                        shake:FireServer()
                                        stats.shakeCount = stats.shakeCount + 1
                                    end)
                                end
                            end
                        end
                    end
                end

                -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥
                statsLabel:Set("üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥: Pan: "..stats.panCount.." | Collect: "..stats.collectCount.." | Shake: "..stats.shakeCount)

                -- üìå ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡πà‡∏≤‡∏´‡∏•‡∏≠‡∏î + ‡∏ß‡∏≤‡∏£‡πå‡∏õ
                local gui = LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("ToolUI")
                if gui and gui:FindFirstChild("FillingPan") and gui.FillingPan:FindFirstChild("FillText") then
                    local text = gui.FillingPan.FillText.Text
                    local a, b = text:match("([%d%.]+)/([%d%.]+)")
                    local current, max = tonumber(a), tonumber(b)

                    if current and max and waterPos and farmPos then
                        if current >= max then
                            Char.HumanoidRootPart.CFrame = waterPos
                            statusLabel:Set("üíß ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏•‡πà‡∏≠‡∏ô‡πÅ‡∏£‡πà (‡πÄ‡∏ï‡πá‡∏°)")
                        else
                            Char.HumanoidRootPart.CFrame = farmPos
                            statusLabel:Set("‚õèÔ∏è ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ü‡∏≤‡∏£‡πå‡∏° ("..current.."/"..max..")")
                        end
                    else
                        statusLabel:Set("‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°/‡∏ô‡πâ‡∏≥‡∏Å‡πà‡∏≠‡∏ô")
                    end
                end
            end

            if not _G.AutoPanWarp then
                statusLabel:Set("‚èπÔ∏è ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô")
            end
        end)
    end
})

-- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥
Tab_Main:CreateButton({
    Name = "üîÑ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥",
    Callback = function()
        stats.panCount = 0
        stats.collectCount = 0
        stats.shakeCount = 0
        statsLabel:Set("üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥: Pan: 0 | Collect: 0 | Shake: 0")
    end
})

------------------------------------------------------------
-- üí∞ ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ç‡∏≤‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÉ‡∏´‡∏°‡πà)
------------------------------------------------------------
Tab_Main:CreateToggle({
    Name = "üí∞ ‡∏Ç‡∏≤‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥",
    CurrentValue = false,
    Callback = function(state)
        _G.AutoSell = state
        task.spawn(function()
            while _G.AutoSell and task.wait(5) do
                local ReplicatedStorage = game:GetService("ReplicatedStorage")
                local SellRemote = ReplicatedStorage:FindFirstChild("Remotes")
                if SellRemote then
                    SellRemote = SellRemote:FindFirstChild("Misc")
                    if SellRemote then
                        SellRemote = SellRemote:FindFirstChild("SellAll")
                        if SellRemote then
                            pcall(function()
                                SellRemote:FireServer()
                                print("üí∞ ‡∏Ç‡∏≤‡∏¢‡∏Ç‡∏≠‡∏á‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥")
                            end)
                        end
                    end
                end
            end
        end)
    end
})

------------------------------------------------------------
-- ‚¨ÜÔ∏è ‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÉ‡∏´‡∏°‡πà)
------------------------------------------------------------
Tab_Main:CreateToggle({
    Name = "‚¨ÜÔ∏è ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥",
    CurrentValue = false,
    Callback = function(state)
        _G.AutoUpgrade = state
        task.spawn(function()
            while _G.AutoUpgrade and task.wait(10) do
                local ReplicatedStorage = game:GetService("ReplicatedStorage")
                local UpgradeRemote = ReplicatedStorage:FindFirstChild("Remotes")
                if UpgradeRemote then
                    UpgradeRemote = UpgradeRemote:FindFirstChild("Misc")
                    if UpgradeRemote then
                        UpgradeRemote = UpgradeRemote:FindFirstChild("Upgrade")
                        if UpgradeRemote then
                            -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î‡πÅ‡∏Ñ‡πà Pan (‡∏´‡∏£‡∏∑‡∏≠‡∏õ‡∏£‡∏±‡∏ö‡πÅ‡∏Å‡πâ‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£)
                            pcall(function()
                                UpgradeRemote:FireServer("Pan") -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á
                                print("‚¨ÜÔ∏è ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î Pan ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥")
                            end)
                        end
                    end
                end
            end
        end)
    end
})

------------------------------------------------------------
-- ‚è≥ ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î (AFK) - ‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á
------------------------------------------------------------
Tab_Main:CreateToggle({
    Name = "‚è≥ ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏´‡∏•‡∏∏‡∏î (Anti AFK)",
    CurrentValue = false,
    Callback = function(state)
        _G.AntiAFK = state
        if state then
            local VirtualInputManager = game:GetService("VirtualInputManager")
            task.spawn(function()
                while _G.AntiAFK and task.wait(60) do
                    -- ‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡πá‡∏ô‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö
                    local keys = {Enum.KeyCode.LeftShift, Enum.KeyCode.W, Enum.KeyCode.A, Enum.KeyCode.S, Enum.KeyCode.D}
                    local randomKey = keys[math.random(1, #keys)]
                    
                    VirtualInputManager:SendKeyEvent(true, randomKey, false, game)
                    task.wait(0.1)
                    VirtualInputManager:SendKeyEvent(false, randomKey, false, game)
                    print("üõ°Ô∏è Anti AFK: ‡∏Å‡∏î", randomKey.Name)
                end
            end)
        end
    end
})

------------------------------------------------------------
-- üèÉ ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß ‡πÅ‡∏•‡∏∞ ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î
