------------------------------------------------------------
-- ü•Ñ ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á)
------------------------------------------------------------
statusLabel = Tab_Main:CreateLabel({
    Text = "‚ö†Ô∏è ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏£‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô...",
    Style = 2
})

local stats = {
    panCount = 0,
    collectCount = 0,
    shakeCount = 0
}

local statsLabel = Tab_Main:CreateLabel({
    Text = "üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥: Pan: 0 | Collect: 0 | Shake: 0",
    Style = 1
})

Tab_Main:CreateToggle({
    Name = "ü•Ñ ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÅ‡∏û‡∏ô + ‡∏ß‡∏≤‡∏£‡πå‡∏õ)",
    CurrentValue = false,
    Callback = function(state)
        _G.AutoPanWarp = state
        task.spawn(function()
            local function isPan(toolName)
                return string.sub(toolName, -3):lower() == "pan"
            end

            while _G.AutoPanWarp and task.wait(_G.AutoSpeed or 1) do
                Char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
                local containers = {Char, LocalPlayer.Backpack}
                for _, container in pairs(containers) do
                    for _, tool in pairs(container:GetChildren()) do
                        if tool:IsA("Tool") and isPan(tool.Name) then
                            local scriptsFolder = tool:FindFirstChild("Scripts")
                            if scriptsFolder then
                                local collect = scriptsFolder:FindFirstChild("Collect")
                                local pan = scriptsFolder:FindFirstChild("Pan")
                                local shake = scriptsFolder:FindFirstChild("Shake")

                                local gui = LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("ToolUI")
                                local fillText = gui and gui:FindFirstChild("FillingPan") and gui.FillingPan:FindFirstChild("FillText")

                                local current, max = nil, nil
                                if fillText and fillText.Text then
                                    local a, b = fillText.Text:match("([%d%.]+)/([%d%.]+)")
                                    current, max = tonumber(a), tonumber(b)
                                end

                                if collect and collect:IsA("RemoteFunction") then
                                    pcall(function()
                                        collect:InvokeServer(1)
                                        stats.collectCount = stats.collectCount + 1
                                    end)
                                end

                                if pan and pan:IsA("RemoteFunction") and current and max and current >= max then
                                    pcall(function()
                                        pan:InvokeServer()
                                        stats.panCount = stats.panCount + 1
                                    end)
                                end

                                if shake and shake:IsA("RemoteEvent") then
                                    pcall(function()
                                        shake:FireServer()
                                        stats.shakeCount = stats.shakeCount + 1
                                    end)
                                end
                            end
                        end
                    end
                end

                statsLabel:Set("üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥: Pan: "..stats.panCount.." | Collect: "..stats.collectCount.." | Shake: "..stats.shakeCount)

                local gui = LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("ToolUI")
                if gui and gui:FindFirstChild("FillingPan") and gui.FillingPan:FindFirstChild("FillText") then
                    local text = gui.FillingPan.FillText.Text
                    local a, b = text:match("([%d%.]+)/([%d%.]+)")
                    local current, max = tonumber(a), tonumber(b)

                    if current and max and waterPos and farmPos then
                        if current >= max then
                            Char.HumanoidRootPart.CFrame = waterPos
                            statusLabel:Set("üíß ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏•‡πà‡∏≠‡∏ô‡πÅ‡∏£‡πà (‡πÄ‡∏ï‡πá‡∏°)")
                        else
                            Char.HumanoidRootPart.CFrame = farmPos
                            statusLabel:Set("‚õèÔ∏è ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ü‡∏≤‡∏£‡πå‡∏° ("..current.."/"..max..")")
                        end
                    else
                        statusLabel:Set("‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏à‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°/‡∏ô‡πâ‡∏≥‡∏Å‡πà‡∏≠‡∏ô")
                    end
                end
            end

            if not _G.AutoPanWarp then
                statusLabel:Set("‚èπÔ∏è ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô")
            end
        end)
    end
})

Tab_Main:CreateButton({
    Name = "üîÑ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥",
    Callback = function()
        stats.panCount = 0
        stats.collectCount = 0
        stats.shakeCount = 0
        statsLabel:Set("üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥: Pan: 0 | Collect: 0 | Shake: 0")
    end
})

------------------------------------------------------------
-- ‚è≥ ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î (AFK)
------------------------------------------------------------
Tab_Main:CreateToggle({
    Name = "‚è≥ ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏´‡∏•‡∏∏‡∏î (Anti AFK)",
    CurrentValue = false,
    Callback = function(state)
        _G.AntiAFK = state
        if state then
            local VirtualInputManager = game:GetService("VirtualInputManager")
            task.spawn(function()
                while _G.AntiAFK and task.wait(60) do
                    local keys = {Enum.KeyCode.LeftShift, Enum.KeyCode.W, Enum.KeyCode.A, Enum.KeyCode.S, Enum.KeyCode.D}
                    local randomKey = keys[math.random(1, #keys)]
                    VirtualInputManager:SendKeyEvent(true, randomKey, false, game)
                    task.wait(0.1)
                    VirtualInputManager:SendKeyEvent(false, randomKey, false, game)
                    print("üõ°Ô∏è Anti AFK: ‡∏Å‡∏î", randomKey.Name)
                end
            end)
        end
    end
})

------------------------------------------------------------
-- üèÉ ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß ‡πÅ‡∏•‡∏∞ ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î (‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏´‡∏•‡∏±‡∏á‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà)
------------------------------------------------------------
local Humanoid = Char:WaitForChild("Humanoid")

Tab_Main:CreateSlider({
    Name = "üèÉ ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß (WalkSpeed)",
    Range = {16, 200},
    Increment = 1,
    CurrentValue = Humanoid.WalkSpeed,
    Callback = function(value)
        if Char and Char:FindFirstChild("Humanoid") then
            Char.Humanoid.WalkSpeed = value
            _G.SavedWalkSpeed = value
        end
    end
})

Tab_Main:CreateSlider({
    Name = "ü¶ò ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á (JumpPower)",
    Range = {50, 500},
    Increment = 5,
    CurrentValue = Humanoid.JumpPower,
    Callback = function(value)
        if Char and Char:FindFirstChild("Humanoid") then
            Char.Humanoid.JumpPower = value
            _G.SavedJumpPower = value
        end
    end
})

LocalPlayer.CharacterAdded:Connect(function(newChar)
    Char = newChar
    task.wait(1)
    if Char:FindFirstChild("Humanoid") then
        Char.Humanoid.WalkSpeed = _G.SavedWalkSpeed or 16
        Char.Humanoid.JumpPower = _G.SavedJumpPower or 50
    end
end)

------------------------------------------------------------
-- ‚ú® ESP Players ‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏°‡πà‡∏ã‡πâ‡∏≠‡∏ô‡∏Å‡∏±‡∏ô
------------------------------------------------------------
local Tab_Visual = Window:CreateTab({
    Name = "‡∏†‡∏≤‡∏û‡πÅ‡∏•‡∏∞‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô",
    Icon = "visibility",
    ImageSource = "Material"
})

Tab_Visual:CreateToggle({
    Name = "üåü FullBright",
    CurrentValue = false,
    Callback = function(state)
        local Lighting = game:GetService("Lighting")
        if state then
            Lighting.Brightness = 2
            Lighting.ClockTime = 14
            Lighting.FogEnd = 100000
            Lighting.GlobalShadows = false
            Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
        else
            Lighting.Brightness = 1
            Lighting.ClockTime = 12
            Lighting.FogEnd = 100000
            Lighting.GlobalShadows = true
            Lighting.OutdoorAmbient = Color3.fromRGB(70, 70, 70)
        end
    end
})

Tab_Visual:CreateToggle({
    Name = "üéØ ESP Players",
    CurrentValue = false,
    Callback = function(state)
        _G.PlayerESP = state
        task.spawn(function()
            local function createESP(player)
                if player == LocalPlayer then return end
                local function addESP(character)
                    if character:FindFirstChild("HumanoidRootPart") then
                        local existing = character.HumanoidRootPart:FindFirstChild("ESP")
                        if existing then existing:Destroy() end
                        local BillboardGui = Instance.new("BillboardGui")
                        BillboardGui.Name = "ESP"
                        local TextLabel = Instance.new("TextLabel")
                        BillboardGui.Parent = character.HumanoidRootPart
                        BillboardGui.Size = UDim2.new(0, 100, 0, 50)
                        BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
                        BillboardGui.AlwaysOnTop = true
                        TextLabel.Parent = BillboardGui
                        TextLabel.Size = UDim2.new(1, 0, 1, 0)
                        TextLabel.BackgroundTransparency = 1
                        TextLabel.Text = player.Name
                        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                        TextLabel.TextStrokeTransparency = 0
                        TextLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
                        TextLabel.Font = Enum.Font.GothamBold
                        TextLabel.TextScaled = true
                    end
                end
                if player.Character then addESP(player.Character) end
                player.CharacterAdded:Connect(addESP)
            end
            if state then
                for _, player in pairs(Players:GetPlayers()) do createESP(player) end
                Players.PlayerAdded:Connect(createESP)
            else
                for _, player in pairs(Players:GetPlayers()) do
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        local esp = player.Character.HumanoidRootPart:FindFirstChild("ESP")
                        if esp then esp:Destroy() end
                    end
                end
            end
        end)
    end
})

