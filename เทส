    Name = "CK HUB ",
    Subtitle = "V.2.15",
    LoadingEnabled = true,
    LoadingTitle = "CK HUB",
    LoadingSubtitle = "‡πÇ‡∏î‡∏¢ fb ‡∏°‡∏≤‡∏õ‡∏±‡πà‡∏ô‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢",
    ConfigSettings = {
        ConfigFolder = "CK HUB Config"
    },
    KeySystem = false
})

Window:CreateHomeTab({
    SupportedExecutors = {"Synapse X", "Krnl", "Fluxus", "Script-Ware"},
    Icon = 1
})

local Tab_Main = Window:CreateTab({
    Name = "‡πÄ‡∏°‡∏ô‡∏π‡∏´‡∏•‡∏±‡∏Å",
