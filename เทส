    Callback = function()
        setclipboard("discord.gg/your_server")
        Luna:CreateNotification({Name="‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß",Content="‡∏•‡∏¥‡∏á‡∏Å‡πå Discord ‡∏ñ‡∏π‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß!",Image="check",Time=3})
