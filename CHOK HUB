-- ตรวจสอบ PlaceId ปัจจุบันก่อน
print("กำลังตรวจสอบ PlaceId:", game.PlaceId)

-- PlaceId ที่เป็นไปได้สำหรับ Fisch
local fischPlaceIds = {
    131716211654599,
    -- เพิ่ม PlaceId อื่นๆ ที่เป็นไปได้
}

local function isInFisch()
    for _, placeId in pairs(fischPlaceIds) do
        if game.PlaceId == placeId then
            return true
        end
    end
    return false
end

if isInFisch() then
    print("พบเกม Fisch! กำลังโหลดสคริปต์...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/chok880/CCF-I-/main/Fisch%20Script", true))()
else
    warn("PlaceId ปัจจุบัน:", game.PlaceId, "ไม่ใช่เกม Fisch")
end
